// ADD
BUILTIN(__builtin_tilegx_v1add, "V8ScV8ScV8Sc", "n")
BUILTIN(__builtin_tilegx_v2add, "V4SsV4SsV4Ss", "n")
BUILTIN(__builtin_tilegx_v4add, "V2SiV2SiV2Si", "n")
// SUB
BUILTIN(__builtin_tilegx_v1sub, "V8ScV8ScV8Sc", "n")
BUILTIN(__builtin_tilegx_v2sub, "V4SsV4SsV4Ss", "n")
BUILTIN(__builtin_tilegx_v4sub, "V2SiV2SiV2Si", "n")
#undef BUILTIN
